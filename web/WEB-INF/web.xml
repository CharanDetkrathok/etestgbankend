<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <filter>
        <filter-name>ThaiRequest</filter-name>
        <filter-class>com.e_exam.filter.ThaiRequest</filter-class>
    </filter>
    <filter-mapping>
        <filter-name>ThaiRequest</filter-name>
        <url-pattern>/*</url-pattern>
    </filter-mapping>
    <servlet>
        <servlet-name>NewServlet</servlet-name>
        <servlet-class>com.et.control.NewServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Login</servlet-name>
        <servlet-class>com.et.login.Login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GetAdminCounter</servlet-name>
        <servlet-class>com.et.control.GetAdminCounter</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EditCounter</servlet-name>
        <servlet-class>com.et.control.EditCounter</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ShowEditSeat</servlet-name>
        <servlet-class>com.et.control.ShowEditSeat</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ShowSeat</servlet-name>
        <servlet-class>com.et.control.ShowSeat</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SaveFile</servlet-name>
        <servlet-class>com.et.control.SaveFile</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ShowSeatDetail</servlet-name>
        <servlet-class>com.et.control.ShowSeatDetail</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GenerateSeat</servlet-name>
        <servlet-class>com.et.control.GenerateSeat</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ExportETSTDC</servlet-name>
        <servlet-class>com.et.control.ExportETSTDC</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GenETSTDC</servlet-name>
        <servlet-class>com.et.control.genfiles.GenETSTDC</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>NewServlet</servlet-name>
        <url-pattern>/NewServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Login</servlet-name>
        <url-pattern>/Login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GetAdminCounter</servlet-name>
        <url-pattern>/GetAdminCounter</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EditCounter</servlet-name>
        <url-pattern>/EditCounter</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ShowEditSeat</servlet-name>
        <url-pattern>/ShowEditSeat</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ShowSeat</servlet-name>
        <url-pattern>/ShowSeat</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SaveFile</servlet-name>
        <url-pattern>/SaveFile</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ShowSeatDetail</servlet-name>
        <url-pattern>/ShowSeatDetail</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GenerateSeat</servlet-name>
        <url-pattern>/GenerateSeat</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ExportETSTDC</servlet-name>
        <url-pattern>/ExportETSTDC</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GenETSTDC</servlet-name>
        <url-pattern>/GenETSTDC</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
